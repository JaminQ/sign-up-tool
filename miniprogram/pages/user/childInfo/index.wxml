<view class="page">
  <view class="weui-cells__title"></view>
  <view wx:if="{{ childInfo.length }}" class="weui-cells weui-cells_after-title">
    <navigator url="addChild?_id={{ _id }}&name={{ item }}&idx={{ index }}" class="weui-cell weui-cell_access" hover-class="weui-cell_active" wx:for="{{ childInfo }}" wx:key="">
      <view class="weui-cell__bd">{{ item }}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="addChild?_id={{ _id }}" class="weui-cell weui-cell_link">
      <view class="weui-cell__bd">添加</view>
    </navigator>
  </view>
</view>