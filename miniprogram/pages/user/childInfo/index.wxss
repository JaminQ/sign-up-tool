movable-area.weui-cell {
  width: 130vw;
  height: 88rpx;
  padding: 0;
  left: -15vw;
}

movable-view {
  display: flex;
  width: 115vw;
  height: 88rpx;
  left: 15vw;
}

navigator {
  position: relative;
  box-sizing:border-box;
  width: 100vw;
  padding: 20rpx 30rpx;
}

.weui-cell__ft_in-access {
  top: -20rpx;
}

.weui-cell.weui-cell_access:before {
  content: "";
  position: absolute;
  margin-left: 30rpx;
  left: 15vw;
  top: 0;
  right: 0;
  height: 2rpx;
  border-top: 1rpx solid #d9d9d9;
  color: #d9d9d9;
}

.del-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 15vw;
  color: #fff;
  background-color: #ff3725;
}