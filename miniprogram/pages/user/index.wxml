<view class="page with-tab-bar">
  <view class="user-avatar">
    <open-data type="userAvatarUrl"></open-data>
  </view>
  <view class="user-nickname">
    <open-data type="userNickName"></open-data>
  </view>

  <view class="weui-cells__title"></view>
  <view class="weui-cells weui-cells_after-title">
    <navigator url="userInfo/index" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">个人信息</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="childInfo/index" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">孩子信息</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="class" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">查看已报名的课程</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>

  <view class="weui-cells__title">管理员功能</view>
  <view class="weui-cells weui-cells_after-title">
    <navigator url="../class/index" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">管理课程</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>
</view>